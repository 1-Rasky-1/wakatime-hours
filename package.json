  "description": "Generate a badge that shows actual hours spent programming using wakatime",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development npx ts-node index.ts",
    "start": "NODE_ENV=production npx ts-node index.ts",
    "format": "prettier --write . --ignore-unknown --cache",
    "format:check": "prettier --check --cache .",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IgorKowalczyk/wakatime-hours.git"
  },
  "keywords": [
    "wakatime",
    "expressjs",
    "typescript",
    "badges"
  ],
  "author": "Igor Kowalczyk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IgorKowalczyk/wakatime-hours/issues"
  },
  "homepage": "https://github.com/IgorKowalczyk/wakatime-hours#readme",
  "dependencies": {
    "@types/compression": "^1.7.2",
    "@types/morgan": "^1.9.4",
    "badge-maker": "3.3.1",
    "chalk": "5.2.0",
    "compression": "1.7.4",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "express-rate-limit": "6.7.0",
    "node-fetch": "3.3.1"
  },
  "devDependencies": {
    "@igorkowalczyk/eslint-config": "1.0.3",
    "@igorkowalczyk/prettier-config": "1.0.3",
    "@types/express": "4.17.17",
    "@types/node": "18.15.6",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "eslint": "8.36.0",
    "morgan": "1.10.0",
    "prettier": "2.8.7",
    "ts-node": "10.9.1",
    "typescript": "5.0.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.30.2"
}
